{
    "name":"Edge Hack",
    "version":"1.0",
    "manifest_version":3,
    "action":{
        "default_popup":"index.html"
    },
    "background": {
      "service_worker": "script.js"
    },
    "host_permissions": [
        "<all_urls>"
      ],
      "permissions": [
        "scripting",
        "storage",
        "tabs",
        "activeTab"
      ]
}